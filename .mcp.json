{
  "mcpServers": {
    "playwright": {
      "command": "npx",
      "args": [
        "@playwright/mcp@latest",
        "--browser",
        "chromium",
        "--headless"
      ]
    },
    "mim": {
      "type": "stdio",
      "command": "node",
      "args": [
        ".claude/servers/mim.cjs"
      ]
    }
  }
}
